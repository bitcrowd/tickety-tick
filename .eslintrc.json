{
  "root": true,
  "extends": ["airbnb", "plugin:jest/recommended"],
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "COMMITHASH": "readonly"
  },
  "plugins": ["jest"],
  "rules": {
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "specialLink": ["to"]
      }
    ],
    "react/jsx-props-no-spreading": ["off"]
  }
}
