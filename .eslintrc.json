{
  "root": true,
  "extends": ["airbnb", "plugin:jest/recommended"],
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "jsx-a11y/anchor-is-valid": ["error", {
      "specialLink": ["to"]
    }],
    "jsx-a11y/label-has-for": ["error", {
      "required": { "some": ["nesting", "id"] }
    }]
  }
}
