    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/speakingurl": "^13.0.3",
    "@types/webextension-polyfill": "^0.10.0",
    "bootstrap": "5.1.1",
    "copy-text-to-clipboard": "^3.0.1",
    "ky": "^0.28.5",
    "mdast-util-from-adf": "^2.1.0",
    "mdast-util-gfm": "^2.0.0",
