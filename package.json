    "react-dom": "17.0.2",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-textarea-autosize": "^8.3.2",
    "serialize-error": "^9.0.0",
    "speakingurl": "14.0.1",
    "strip-indent": "^4.0.0",
    "webextension-polyfill": "^0.10.0"
  }
}
