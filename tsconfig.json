{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "skipLibCheck": true,
    "strict": true,
    "target": "es5"
  },
  "include": ["./src/**/*", "./test/**/*"]
}
